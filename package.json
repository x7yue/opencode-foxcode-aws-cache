    "type": "git",
    "url": ""
  },
  "peerDependencies": {
    "@opencode-ai/plugin": ">=0.15.0"
  },
  "devDependencies": {
    "@opencode-ai/plugin": "^1.1.21",
    "@types/bun": "^1.3.1"
  }
