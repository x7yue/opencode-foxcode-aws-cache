{
  "name": "opencode-foxcode-aws-cache",
  "version": "1.0.2",
  "description": "OpenCode plugin that enables prompt caching for Foxcode AWS channel by injecting metadata.user_id",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node && bun x tsc --emitDeclarationOnly",
    "prepublishOnly": "bun run build",
    "release": "bun run build && changeset publish --access public",
    "changeset": "changeset"
  },
  "keywords": [
    "opencode",
    "foxcode",
    "aws",
    "anthropic",
    "claude",
    "cache",
    "prompt-caching",
    "plugin"
  ],
  "author": "x7yue",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/x7yue/opencode-foxcode-aws-cache"
  },
  "peerDependencies": {
    "@opencode-ai/plugin": ">=0.15.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@opencode-ai/plugin": "^1.1.21",
    "@types/bun": "^1.3.1"
  }
}
